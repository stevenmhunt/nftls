{
  "name": "nftls",
  "version": "1.0.5",
  "description": "A command-line tool and NodeJS library for working with NFTLS certificates.",
  "bin": {
    "nftls": "./bin/nftls.js",
    "nftls-lookup": "./bin/nftls-lookup.js"
  },
  "scripts": {
    "test": "npx mocha",
    "lint": "npx eslint .",
    "jsdoc": "mkdirp ./build/jsdoc && npx jsdoc ./src README.md -d ./build/jsdoc",
    "browserify": "mkdirp ./build && npx browserify --ignore fs-extra --ignore jimp --ignore ethers --standalone nftls nftls.browser.js -o ./build/nftls.js && npx minify ./build/nftls.js > ./build/nftls.min.js"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "cli-color": "^2.0.1",
    "cli-spinner": "^0.2.10",
    "eth-signature-mark": "^1.1.1",
    "ethers": "^5.5.3",
    "fs-extra": "^10.0.0",
    "jimp": "^0.16.1",
    "joi": "^17.5.0",
    "keccak": "^3.0.1",
    "keccak256": "^1.0.6",
    "lodash": "^4.17.21",
    "minimist": "^1.2.5",
    "mri": "^1.2.0",
    "node-gzip": "^1.1.2",
    "pngjs3": "^6.0.1",
    "randombytes": "^2.0.6",
    "secp256k1": "^3.8.0",
    "tostream": "^1.0.0"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "browserify-fs": "^1.0.0",
    "chai": "^4.3.4",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "jsdoc": "^3.6.7",
    "minify": "^8.0.3",
    "mocha": "^9.1.4"
  }
}
